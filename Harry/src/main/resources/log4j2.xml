<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
	<appenders>
		<Console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="[%d{HH:mm:ss.SSS}][%p][%t|%logger|%M|%X{ctime}] - %msg%n" />
		</Console>
		<Socket name="socket" host="192.168.1.236" port="33333" protocol="UDP"> 
			<PatternLayout pattern="xindai [%d{yyyy-MM-dd HH:mm:ss.SSS}][%p][%X{sessionId}][%X{traceId}][%X{cip}:%X{cPort}][%X{sip}:%X{sPort}][][%X{userId}][%t|%logger{1.}|%M|%X{ctime}] - %msg%n" />
		</Socket>
	</appenders>
	<loggers>
		<logger name="org.apache.zookeeper" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="org.I0Itec.zkclient" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="org.springframework" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="com.alibaba.dubbo" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="org.mybatis" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="org.apache.ibatis" level="error" additivity="false">
			<appender-ref ref="Console" />
		</logger>
		<logger name="net.sf.ehcache" level="warn" additivity="false">
			<appender-ref ref="Console" />
			<appender-ref ref="socket" />
		</logger>
        <logger name="org.apache.shiro" level="debug" additivity="false">
            <appender-ref ref="Console" />
            <appender-ref ref="socket" />
        </logger>
		<root level="DEBUG">
			<appender-ref ref="Console" />
			<!--<appender-ref ref="socket"></appender-ref>-->
		</root>
	</loggers>
</configuration>